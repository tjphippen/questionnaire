{
    "name": "tjphippen/questionnaire",
    "description": "Weighted questionnaire for a laravel 5.8+ application",
    "homepage": "https://github.com/tjphippen/questionnaire",
    "keywords":
    [
        "tjphippen",
        "questionnaire",
        "survey",
        "questions",
        "answers",
        "laravel"
    ],
    "authors": [
        {
          "name": "TJ Phippen",
          "homepage": "http://tjphippen.com",
          "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1",
        "illuminate/config": "~5.8.0",
        "illuminate/database": "~5.8.0",
        "illuminate/support": "~5.8.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "~3.8.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Tjphippen\\Questionnaire\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tjphippen\\Questionnaire\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Tjphippen\\Questionnaire\\QuestionnaireServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
